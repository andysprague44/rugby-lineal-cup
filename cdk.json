{
    "app": "python app.py --context stage=dev",
    "watch": {
      "include": [
        "cdk/**",
        "src/**"
      ],
      "exclude": [
        "README.md",
        "LICENSE",
        "cdk*.json",
        "requirements*.txt",
        "__pycache__",
        ".venv"
      ]
    },
    "context": {
      "@aws-cdk/core:newStyleStackSynthesis": true
    }
  }
  
  